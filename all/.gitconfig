[core]
    pager = delta

[color]
    ui = auto

[init]
    defaultBranch = canon

[interactive]
    diffFilter = delta --color-only

[pull]
    rebase = true

[rebase]
    autoStash = true

[user]
    name = Donald Guy
    email = donaldguy@hey.com

[filter "lfs"]
    required = true
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process

